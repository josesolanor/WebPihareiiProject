                @Html.DisplayNameFor(model => model.DepartmentStateValue)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DepartmentStateDescription)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
