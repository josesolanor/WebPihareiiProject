            <td>
                @Html.DisplayFor(modelItem => item.DepartmentStateValue)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DepartmentStateDescription)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.DepartmentStateId">Edit</a> |
